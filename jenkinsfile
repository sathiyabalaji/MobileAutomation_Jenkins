node{
  stage('SCM Checkout'){
  git 'https://github.com/sathiyabalaji/MobileAutomation_Jenkins'
  }
  stage('Compile-Package'){
    echo "Hello world"
    def mvnHome = tool name: 'Maven-3.6.3', type: 'maven'
    bat """
    "{MAVEN_HOME}/mvn" clean test
    """
    //sh "${mvnHome}/bin/mvn package"
   // sh 'mvn package'
   // mvn clean
  }
}
